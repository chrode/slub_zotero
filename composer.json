{
  "name": "slub/slub-zotero",
  "type": "typo3-cms-extension",
  "description": "This extension should call the zotero api and present the SLUB bibliography on the website.",
  "homepage": "https://github.com/slub/slub_zotero",
  "keywords": [
    "TYPO3",
    "extension",
    "slub_zotero"
  ],
  "authors": [
    {
      "name": "Sebastian Semsker",
      "email": "typo3@slub-dresden.de",
      "role": "Developer"
    },
    {
      "name": "Norman Steger",
      "email": "norman.steger@slub-dresden.de",
      "role": "Developer"
    },
    {
      "name": "Alexander Bigga",
      "email": "typo3@slub-dresden.de",
      "role": "Developer"
    }
  ],
  "license": "GPL-3.0-or-later",
  "require": {
    "typo3/cms-core": "^9.5|^10.4|^11.5"
  },
  "autoload": {
    "psr-4": {
      "Slub\\SlubZotero\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
       "extension-key": "slub_zotero"
    }
 }
}
